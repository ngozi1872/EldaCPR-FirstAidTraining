            <div class="pr-circle10 pr-child"></div>
            <div class="pr-circle11 pr-child"></div>
            <div class="pr-circle12 pr-child"></div>
        </div>
    </div>
    <!-- End Preloader -->
    <!-- Top Bar -->
    <section class="top-bar" id="btt">
        <div class="container">
            <div class="row">
                <div class="col-lg-7 col-md-9">
                    <div class="bar-content">
                        <ul class="list-unstyled list-inline">
                            <li class="list-inline-item"><img src="images/phone.png" alt="">+1 (267) 242-4795</li>
                            <li class="list-inline-item"><img src="images/clock.png" alt="">Mon - Sat 8:30am - 6:30pm</li>
                            <li class="list-inline-item"><img src="images/mail.png" alt="">eleekpalay@aol.com</li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-5 col-md-3">
                    <div class="top-social text-right">
                        <ul class="list-unstyled list-inline">
                            <li class="list-inline-item"><a href=""><i class="fa fa-facebook"></i></a></li>
                            <li class="list-inline-item"><a href=""><i class="fa fa-twitter"></i></a></li>
                            <li class="list-inline-item"><a href=""><i class="fa fa-linkedin"></i></a></li>
                            <li class="list-inline-item"><a href=""><i class="fa fa-google-plus"></i></a></li>
                            <li class="list-inline-item">
                                <span><button type="button" data-toggle="modal" data-target="#model-box">Make an Appointment</button></span>
                                <!-- Appointment Box -->
                                <div class="modal fade" id="model-box">
                                    <div class="modal-dialog modal-dialog-centered">
                                        <div class="modal-content text-center">
                                            <div class="medel-box-top">
                                                <h4>Book An Appointment</h4>
                                                <img src="images/heartbeat.png" alt="">
                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                    <i class="fa fa-times"></i>
                                                </button>
                                            </div>
                                            <form action="#">
                                                <input type="text" name="name" placeholder="Full Name" required>
                                                <input type="text" name="phone" placeholder="Phone Number" required>
                                                <input type="text" name="email" placeholder="Email Address" required>
                                                <input type="text" id="datetimepicker" data-toggle="datetimepicker" data-target="#datetimepicker" name="appdate" placeholder="Appointment Date">
                                                <textarea name="message" placeholder="Message" required></textarea>
                                                <button type="submit">Book Now</button>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End Top Bar -->
    <!-- Logo Area -->
    <section class="logo-area menu-sticky">
        <div class="container">
            <div class="row">
                <div class="col-md-3">
