                            </li>
                            <li class="list-inline-item">
                                <a>SERVICE</a>
                            </li>
                            <li class="list-inline-item">
                                <a>ABOUT US</a>
                            </li>
                            <li class="list-inline-item">
                                <a>APPOINTMENT</a>
                            </li>
                            <li class="list-inline-item mega-menu">
                                <a>CONTACT</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End Logo Area -->
